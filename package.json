{
  "name": "service-platform-backend",
  "version": "1.0.0",
  "description": "Multi-tenant service booking platform",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "migrate:indexes": "node scripts/migrations/create_indexes.js",
    "setup": "node scripts/setup-initial-data.js",
    "verify": "bash scripts/verify-system.sh",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f backend",
    "docker:build": "docker-compose up -d --build",
    "docker:clean": "docker-compose down -v",
    "lint": "echo 'No linter configured yet'",
    "format": "echo 'No formatter configured yet'"
  },
  "dependencies": {
    "@socket.io/redis-adapter": "^8.2.1",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.3.1",
    "firebase-admin": "^13.6.0",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.1",
    "pdfkit": "^0.13.0",
    "razorpay": "^2.9.2",
    "redis": "^4.6.11",
    "socket.io": "^4.6.1",
    "twilio": "^4.23.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  }
}
